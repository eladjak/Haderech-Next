  "engines": {
    "node": ">=18.x",
    "npm": ">=9.x"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "pnpm run type-check",
      "pnpm run lint:fix",
      "pnpm run format",
      "vitest related --run"
    ],
    "*.{json,md,mdx,css,scss,yml,yaml}": [
      "pnpm run format"
    ],
    "package.json": [
      "pnpm run format",
      "pnpm dedupe"
    ]
  }
